#: ../wp-widget-sub-post-navigation.php:27
msgid "Sub Post Navigation"
msgstr "Hierarchische Navigation"

#: ../wp-widget-sub-post-navigation.php:28
msgid "This widget display a navigation on hierarchically organized post types."
msgstr "Dieses Widget zeigt eine Navigation für hierarchisch organisierte Artikel."

#: ../wp-widget-sub-post-navigation.php:62
msgid "Navigation"
msgstr "Navigation"

#: ../wp-widget-sub-post-navigation.php:194
#, php-format
msgid "Sorry, this feature is only available in the <a href=\"%s\" target=\"_blank\">Pro Version</a>."
msgstr "Entschuldigung, diese Funktion gibt es nur in der <a href=\"%s\" target=\"_blank\">Pro Version</a>."

#: ../wp-widget-sub-post-navigation.php:195
msgctxt "Link to the authors website"
msgid "http://dennishoppe.de/en/wordpress-plugins/sub-post-navigation"
msgstr "http://dennishoppe.de/wordpress-plugins/sub-post-navigation"

#: ../form.php:3
msgid "Title:"
msgstr "Titel:"

#: ../form.php:5
msgid "This title will only be used if there is no useable parent post title."
msgstr "Der Titel wird nur verwendet wenn es keinen brauchbaren übergeordneten Titel gibt."

#: ../form.php:11
msgid "Hide the widget title."
msgstr "Widget-Titel verstecken."

#: ../form.php:18
msgid "Replace the widget title with the title of the parent page if possible."
msgstr "Ersetze den Widget-Titel mit dem Titel der Elternseite falls möglich."

#: ../form.php:22
msgid "Post Types"
msgstr "Inhaltstypen"

#: ../form.php:23
msgid "Please select the post types which should be shown by this widget."
msgstr "Bitte wählen Sie die Inhaltstypen für die Sie dieses Widget benutzen möchten."

#: ../form.php:34
msgid "Post order"
msgstr "Artikelreihenfolge"

#: ../form.php:36
msgid "Order posts by:"
msgstr "Sortieren nach:"

#: ../form.php:38
msgid "Post ID"
msgstr "Artikel ID"

#: ../form.php:39
msgid "Author"
msgstr "Autor"

#: ../form.php:40
msgid "Title"
msgstr "Titel"

#: ../form.php:41
msgid "Date"
msgstr "Datum"

#: ../form.php:42
msgid "Modified date"
msgstr "Bearbeitungsdatum"

#: ../form.php:43
msgid "Random order"
msgstr "Zufallsreihenfolge"

#: ../form.php:44
msgid "Number of comments"
msgstr "Kommentaranzahl"

#: ../form.php:45
msgid "Menu order"
msgstr "Menüreihenfolge"

#: ../form.php:52
msgid "Order the posts ascending."
msgstr "Artikel aufsteigend sortieren."

#: ../form.php:59
msgid "Order the posts descending."
msgstr "Artikel absteigend sortieren."

#: ../form.php:66
msgid "Exclude"
msgstr "Ausschliessen"

#: ../form.php:76
msgid "Special case"
msgstr "Sonderfall"

#: ../form.php:80
msgid "Hide this widget on top level posts if there are no child posts."
msgstr "Zeige das Widget nicht auf Top Level Beiträgen wenn es keine Unterobjekte gibt."

#: ../form.php:85
msgid "Parent link"
msgstr "Aufwärts-Link"

#: ../form.php:87
msgid "Hide the parent post link."
msgstr "Link zum übergeordneten Artikel verstecken."

#: ../form.php:94
msgid "Show the parent post link on top of the list."
msgstr "Zeige den Aufwärts-Link über der Navigation."

#: ../form.php:101
msgid "Show the parent post link on bottom of the list."
msgstr "Zeige den Aufwärts-Link unter der Navigation."

