#: ../wp-widget-sub-post-navigation.php:27
msgid "Sub Post Navigation"
msgstr "Undertype indlægs navigation"

#: ../wp-widget-sub-post-navigation.php:28
msgid "This widget display a navigation on hierarchically organized post types."
msgstr "Denne widget viser en navigation af hierarkisk organiseret indlæg efter typer."

#: ../wp-widget-sub-post-navigation.php:62
msgid "Navigation"
msgstr "Navigation"

#: ../wp-widget-sub-post-navigation.php:194
#, php-format
msgid "Sorry, this feature is only available in the <a href=\"%s\" target=\"_blank\">Pro Version</a>."
msgstr "Beklager, denne feature er kun tilgænelig i <a href=\"%s\" target=\"_blank\">Pro Versionen</a>."

#: ../wp-widget-sub-post-navigation.php:195
msgctxt "Link to the authors website"
msgid "http://dennishoppe.de/en/wordpress-plugins/sub-post-navigation"
msgstr "http://dennishoppe.de/en/wordpress-plugins/sub-post-navigation"

#: ../form.php:3
msgid "Title:"
msgstr "Titel:"

#: ../form.php:5
msgid "This title will only be used if there is no useable parent post title."
msgstr "Denne titel vil kun blive brugt, hvis der ikke er nogen brugbar forældre indlægs titel."

#: ../form.php:11
msgid "Hide the widget title."
msgstr "Skjul widget titlen"

#: ../form.php:18
msgid "Replace the widget title with the title of the parent page if possible."
msgstr "Udskift widget tilen med titlen fra forældre siden, hvis muligt."

#: ../form.php:22
msgid "Post Types"
msgstr "Indlægs type"

#: ../form.php:23
msgid "Please select the post types which should be shown by this widget."
msgstr "Vælg venligst den indlægs type der skal vises af denne widget."

#: ../form.php:34
msgid "Post order"
msgstr "Indlægs rækkefølge"

#: ../form.php:36
msgid "Order posts by:"
msgstr "Vælg indlægs rækkefølge efter:"

#: ../form.php:38
msgid "Post ID"
msgstr "Indlægs ID"

#: ../form.php:39
msgid "Author"
msgstr "Forfatter"

#: ../form.php:40
msgid "Title"
msgstr "Titel"

#: ../form.php:41
msgid "Date"
msgstr "Dato"

#: ../form.php:42
msgid "Modified date"
msgstr "Ændrings dato"

#: ../form.php:43
msgid "Random order"
msgstr "Tilfældig rækkefølge"

#: ../form.php:44
msgid "Number of comments"
msgstr "Antal kommentar"

#: ../form.php:45
msgid "Menu order"
msgstr "Menu rækkefølge"

#: ../form.php:52
msgid "Order the posts ascending."
msgstr "Indlæg i stigende orden."

#: ../form.php:59
msgid "Order the posts descending."
msgstr "Indlæg i faldende orden."

#: ../form.php:66
msgid "Exclude"
msgstr "Udelukke"

#: ../form.php:76
msgid "Special case"
msgstr "Særlige tilfælde"

#: ../form.php:80
msgid "Hide this widget on top level posts if there are no child posts."
msgstr "Skujl denne widget på 'forældre' indlæg, hvis der ikke er nogen 'child' indlæg."

#: ../form.php:85
msgid "Parent link"
msgstr "Forældre link"

#: ../form.php:87
msgid "Hide the parent post link."
msgstr "Skjul forældre indlægs linket."

#: ../form.php:94
msgid "Show the parent post link on top of the list."
msgstr "Vis forældre indlægs link i toppen af listen."

#: ../form.php:101
msgid "Show the parent post link on bottom of the list."
msgstr "Vis forældre indlægs linket i bunden af listen."

